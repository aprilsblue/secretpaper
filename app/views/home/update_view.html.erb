<div class="row">
    <div class="col-md-4"></div>
    <div class="container col-md-4">
        수정하기 페이지
        <form method="POST" action="/update/<%= @one_paper.id %>">
          <div class="form-group">
            <label class="title" for="exampleInputNickName">Nickname</label>
            <input name="nickname" id="nickname" type="text" class="form-control" value="<%= @one_paper.nickname %>" disabled="disabled"/>
            <label class="title" for="exampleInputStroy">Story</label>
            <textarea name="content" id="content" class="form-control" rows=10><%= @one_paper.content %></textarea>
          </div>
          <button type="submit" class="btn btn-default btn-lg btn-block">Update</button>
          <button type="button" id="reply" class="btn btn-success btn-lg btn-block" onclick="location.href='/display'">Back</button>
        </form>
    </div>
    <div class="col-md-4"></div>
</div>